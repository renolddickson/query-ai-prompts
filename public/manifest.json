{
    "manifest_version": 3,
    "name": "Query #section-id",
    "version": "1.0",
    "description": "Queries and displays all #section-id elements from the current page",
    "permissions": ["scripting", "activeTab"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }